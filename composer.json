{
    "name": "botovis/botovis-telegram",
    "description": "Botovis Telegram Channel â€” Control your database from Telegram using AI",
    "keywords": ["botovis", "telegram", "bot", "ai", "smart-layer", "natural-language"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "botovis/core": "^0.1",
        "botovis/botovis-laravel": "^0.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Botovis\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Botovis\\Telegram\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core",
            "options": {
                "versions": {
                    "botovis/core": "0.1.99"
                }
            }
        },
        {
            "type": "path",
            "url": "../laravel",
            "options": {
                "versions": {
                    "botovis/botovis-laravel": "0.1.99"
                }
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Botovis\\Telegram\\TelegramServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
